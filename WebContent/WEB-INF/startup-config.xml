<?xml version="1.0" encoding="UTF-8"?>

<startup-config>
	<setup>
		<complete>false</complete>
	</setup>
	<!--LICENSE SETTING HERE! -->
	<license />
	<!--SECURITY SETTING HERE! -->
	<security>
		<encrypt>
			<algorithm>Blowfish</algorithm>
			<key>
				<current />
			</key>
			<property>
				<name>username</name>
				<name>password</name>
			</property>
		</encrypt>
		<authentication>
			<encoding>
				<algorithm>SHA-256</algorithm>
			</encoding>
		</authentication>
	</security>
	<!--SERVICES SETTING HERE ! -->
	<services>
		<!--config service -->
		<config>
			<persistence>
				<jdbc>
					<enabled>true</enabled>
				</jdbc>
			</persistence>
			<tables />
		</config>
		<!--sqlquery service -->
		<sqlquery>
			<resource>
				<location>sql</location>
			</resource>
		</sqlquery>
		<groovy>
			<path>groovy-script</path>
		</groovy>
		<setup>
			<datasources>
				<enabled>true</enabled>
			</datasources>
		</setup>
	</services>
	<!--COMPONENTS SETTING HERE! -->
	<!--DATABASE SETTING -->
	<database>
		<default>
			<beanName>dataSource</beanName>
			<pooledDataSourceProvider>
				<driverClassName>com.mysql.cj.jdbc.Driver</driverClassName> 
				<url>jdbc:mysql://xxx.xxx.xxx.xxx:3306/islanddb?serverTimezone=Asia/Seoul</url>
				<username>xxxx</username>
				<password>xxxx</password>
				<connectionProperties>
					<initialSize>1</initialSize>
					<maxActive>8</maxActive>
					<maxIdle>8</maxIdle>
					<maxWait>-1</maxWait>
					<minIdle>0</minIdle>
					<testOnBorrow>false</testOnBorrow>
					<testOnReturn>false</testOnReturn>
					<testWhileIdle>false</testWhileIdle>
					<validationQuery>select 1</validationQuery>
				</connectionProperties>
			</pooledDataSourceProvider>
		</default>
	</database>
</startup-config>
